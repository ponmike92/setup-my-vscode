{
	// FONT & THEME
	"editor.fontSize": 14,
	"terminal.integrated.fontSize": 13,
	"scm.inputFontSize": 14,
	"window.zoomLevel": 0,
	"editor.fontFamily": "JetBrainsMono Nerd Font",
	"workbench.colorTheme": "Moegi Black",
	"workbench.iconTheme": "vscode-seedling-icon-theme-plus",
	
	// UPDATES
	"update.mode": "manual",

	// COLORS
	"workbench.colorCustomizations": {
		// cursor
		"editorCursor.foreground": "#eeeeee",

		// widgets
		"quickInput.background": "#181818",

		// terminal
		"terminal.background": "#161616",

		// diff editor
		"diffEditor.removedTextBackground": "#ac2e5620",
		"diffEditor.removedLineBackground": "#ac2e5620",
		"diffEditorGutter.removedLineBackground": "#ac2e5620",
		"diffEditorOverview.removedForeground": "#ac2e56ff",

		"diffEditor.insertedTextBackground": "#4A645940",
		"diffEditor.insertedLineBackground": "#4A645940",
		"diffEditorGutter.insertedLineBackground": "#4A645940",
		"diffEditorOverview.insertedForeground": "#4A6459ff",

		// git
		"editorGutter.modifiedBackground": "#222a3a",
		"editorGutter.deletedBackground": "#ac2e5633",
		"editorGutter.addedBackground": "#263324",

		// bracket match
		"editorBracketMatch.border": "#ffffff00",
		"editorBracketMatch.background": "#ffffff00",

		// interface links
		"textLink.foreground": "#c75f80",
		"textLink.activeForeground": "#e86b92",

		// interface buttons
		"button.background": "#ac2e56",
		"button.foreground": "#FFFFFF",

		// folded
		"editor.foldBackground": "#ffffff00",

		// selection
		"editor.selectionBackground": "#303030",

		// main background
		"editor.background": "#202020",
		"sideBar.background": "#161616",
		"sideBarSectionHeader.background": "#161616",

		// main text in editor
		"editor.foreground": "#ffffffbb",

		// line numbers
		"editorLineNumber.foreground": "#444444",

		// warnings and errors
		"editorError.foreground": "#ff646455",
		"editorWarning.foreground": "#fa973a55",

		// sidebar
		"sideBarTitle.foreground": "#222222",
		"sideBar.foreground": "#484848",
		"sideBarSectionHeader.foreground": "#666666",
		"sideBarSectionHeader.border": "#222222",

		"tab.activeForeground": "#EEEEEE",
		"tab.activeBorder": "#FFFFFFFF",

		"list.activeSelectionForeground": "#EEEEEE",
		"list.inactiveSelectionForeground": "#EEEEEE",
		"list.activeSelectionBackground": "#FFFFFF00",
		"list.inactiveSelectionBackground": "#FFFFFF00",

		"list.hoverBackground": "#FFFFFF00",
		"list.focusBackground": "#FFFFFF02",
		"list.focusOutline": "#FFFFFF00",
		"list.focusForeground": "#DDDDDD",
		"list.hoverForeground": "#EEEEEE",

		"focusBorder": "#FFFFFF00",

		"sideBar.border": "#00000020",
		"icon.foreground": "#222222",

		"editor.lineHighlightBackground": "#5b5b5b02",
		"gitDecoration.modifiedResourceForeground": "#FFFFFF11",
		"gitDecoration.stageModifiedResourceForeground": "#FFFFFF11",
		"gitDecoration.deletedResourceForeground": "#FFFFFF11",
		"gitDecoration.stageDeletedResourceForeground": "#FFFFFF11",
		"gitDecoration.addedResourceForeground": "#FFFFFF11",
		"gitDecoration.untrackedResourceForeground": "#FFFFFF11",
		"gitDecoration.ignoredResourceForeground": "#FFFFFF11",
		"gitDecoration.conflictingResourceForeground": "#FFFFFF11",
		"gitDecoration.renamedResourceForeground": "#FFFFFF11",
		"gitDecoration.submoduleResourceForeground": "#FFFFFF11"
	},

