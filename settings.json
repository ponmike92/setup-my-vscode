					"foreground": "#ffffff55"
				}
			},
			{
				"scope": "punctuation.definition.bold.markdown",
				"settings": {
					"foreground": "#666666"
				}
			},
			{
				"scope": "punctuation.definition.italic.markdown",
				"settings": {
					"foreground": "#666666"
				}
			},
			{
				"scope": [
					"string.quoted.single",
					"string.quoted.double",
					"string.quoted.multi",
					"string.quoted.docstring.multi"
				],
