	// FORMATTING
	// editor
	"editor.formatOnSave": false,
	"editor.formatOnPaste": true,
	"editor.formatOnType": false,
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"editor.tabSize": 4,

	// prittier
	"prettier.printWidth": 160,
	"prettier.semi": false,
	"prettier.useTabs": true,
	"prettier.singleQuote": true,
	"prettier.jsxSingleQuote": true,
	"prettier.arrowParens": "always",

	// ruff
	"ruff.lineLength": 120,

	// JSON & YAML
	"[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	// "[yaml]": { "editor.defaultFormatter": "kennylong.kubernetes-yaml-formatter" },

	// JS & TS
	"[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
	"[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },

	"javascript.updateImportsOnFileMove.enabled": "always",
	"javascript.format.semicolons": "remove",
	"javascript.preferences.quoteStyle": "single",
	"typescript.format.semicolons": "remove",
	"typescript.preferences.quoteStyle": "single",

	// python
	"[python]": {
		"editor.defaultFormatter": "charliermarsh.ruff"
	},
	"python.analysis.autoIndent": false,

	// markdown
	"[markdown]": {
		"editor.detectIndentation": true,
		"editor.insertSpaces": true,
		"editor.tabSize": 2
	},

	// shell
	"[shellscript]": { "editor.defaultFormatter": "mads-hartmann.bash-ide-vscode" },

	// Lua
	"[lua]": { "editor.defaultFormatter": "JohnnyMorganz.stylua" },
	"Lua.diagnostics.enable": false,
	"Lua.completion.autoRequire": false,
	"Lua.completion.enable": false,
	"Lua.language.completeAnnotation": false,
	"Lua.addonManager.enable": false,
	"Lua.completion.showParams": false,
	"Lua.completion.workspaceWord": false,
	"Lua.hint.await": false,
	"Lua.hint.paramType": false,
	"Lua.hover.enable": false,
	"Lua.hover.expandAlias": false,
	"Lua.format.enable": false,
	"Lua.hover.viewNumber": false,
	"Lua.hover.viewString": false,
	"Lua.semantic.annotation": false,
	"Lua.semantic.enable": false,
	"Lua.semantic.variable": false,
	"Lua.signatureHelp.enable": false,
	"Lua.type.castNumberToInteger": false,
	"Lua.window.progressBar": false,
	"Lua.window.statusBar": false,
	"Lua.language.fixIndent": false,
	"Lua.completion.keywordSnippet": "Disable",
	"Lua.completion.showWord": "Disable",
	"Lua.hint.arrayIndex": "Disable",
	"Lua.hint.paramName": "Disable",

	// DEBUG
	"debug.focusWindowOnBreak": false,
	"debug.showInlineBreakpointCandidates": false,
	"debug.showBreakpointsInOverviewRuler": false,
	"security.workspace.trust.untrustedFiles": "open",

	// GIT
	"scm.showActionButton": false,
	"git.decorations.enabled": true,
	"git.timeline.showAuthor": false,
	"git.timeline.showUncommitted": true,
	"git.showCommitInput": false,
	"git.useEditorAsCommitInput": false,
	"git.suggestSmartCommit": false,
	"git.openRepositoryInParentFolders": "never",

	// LANGUAGES

	// WINDOW
	// EDIT ME: 'Contents/Resources/app/out/main.js'
	//   sed -i 's/webPreferences/frame:!1,webPreferences/'
	"window.titleBarStyle": "native",
	"window.commandCenter": false,
	"window.density.editorTabHeight": "compact",
	"window.openFilesInNewWindow": "off",
	"window.nativeTabs": false,
	"window.openFoldersInNewWindow": "off",

	// BREADCRUMBS
	"breadcrumbs.enabled": false,
	"breadcrumbs.icons": false,
	"breadcrumbs.showKeys": false,
	"breadcrumbs.showFiles": false,
	"breadcrumbs.symbolPath": "off",
	"breadcrumbs.showArrays": false,
	"breadcrumbs.showEvents": false,
	"breadcrumbs.showFields": false,
	"breadcrumbs.showClasses": false,
	"breadcrumbs.showMethods": false,
	"breadcrumbs.showBooleans": false,
	"breadcrumbs.showFunctions": false,
	"breadcrumbs.showConstants": false,
	"breadcrumbs.showEnumMembers": false,
	"breadcrumbs.showConstructors": false,
	"breadcrumbs.filePath": "off",

	// WORKBENCH
	// ui
	"workbench.sideBar.location": "right",
	"workbench.statusBar.visible": false,
	"workbench.editor.customLabels.enabled": true,
	"workbench.editor.customLabels.patterns": {
		"**": "${filename}                                                                                                                                                                                                                                                            "
	},
	// file opens
	"workbench.editor.revealIfOpen": true,
	"workbench.editor.enablePreview": false,
	// "workbench.editor.enablePreviewFromQuickOpen": false,

	// redundant
	"workbench.tree.enableStickyScroll": false,
	"workbench.startupEditor": "none",
	"workbench.editor.editorActionsLocation": "hidden",
	"workbench.layoutControl.enabled": false,
	"workbench.editor.showTabs": "none",
	"workbench.editor.showIcons": false,
	"workbench.editor.tabActionCloseVisibility": false,
	"workbench.editor.tabSizing": "shrink",
	"workbench.editor.pinnedTabSizing": "shrink",
	"workbench.editor.labelFormat": "short",
	"workbench.quickOpen.closeOnFocusLost": false,

	// FILES
	"files.exclude": {},
	"files.defaultLanguage": "markdown",
	"files.associations": {
		"*.yazi": "toml",
		"*.sntheme": "json",
		"*.ahcommand": "javascript"
	},
	"files.autoSave": "afterDelay",

	// EXPLORER
	// sorting
	"explorer.openEditors.sortOrder": "editorOrder",
	"explorer.sortOrder": "default",

	// nesting
	"explorer.fileNesting.patterns": {
		"*.ts": "${capture}.js",
		"*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
		"*.jsx": "${capture}.js",
		"*.tsx": "${capture}.ts",
		"tsconfig.json": "tsconfig.*.json",
		"package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
		"*.sqlite": "${capture}.${extname}-*",
		"*.db": "${capture}.${extname}-*",
		"*.sqlite3": "${capture}.${extname}-*",
		"*.db3": "${capture}.${extname}-*",
		"*.sdb": "${capture}.${extname}-*",
		"*.s3db": "${capture}.${extname}-*"
	},

	// etc
	"explorer.openEditors.minVisible": 9,
	"explorer.openEditors.visible": 9,
	"explorer.decorations.badges": true,
	"explorer.decorations.colors": false,
	"explorer.confirmDelete": false,
	"explorer.compactFolders": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.autoReveal": false,
	"explorer.confirmPasteNative": false,

	// EDITOR
	// autocomplete & suggestions
	"editor.inlineSuggest.showToolbar": "never",
	"editor.snippetSuggestions": "none",
	"editor.suggest.showWords": false,
	"editor.suggestOnTriggerCharacters": false,
	"editor.suggestSelection": "first",
	"editor.quickSuggestionsDelay": 0,
	"editor.quickSuggestions": {
		"other": "on",
		"comments": "on",
		"strings": "on"
	},
	"editor.inlineSuggest.suppressSuggestions": false,
	"editor.screenReaderAnnounceInlineSuggestion": false,
	"editor.acceptSuggestionOnEnter": "on",
	"editor.wordBasedSuggestions": "matchingDocuments",

	// word wraps
	"editor.wordWrap": "wordWrapColumn",
	"editor.wrappingIndent": "same",
	"editor.wordWrapColumn": 120,

	// cursor & sticky scroll
	"editor.cursorSurroundingLines": 5,
	"editor.cursorBlinking": "solid",
	"editor.cursorStyle": "underline",
	"editor.cursorSmoothCaretAnimation": "explicit",
	"editor.occurrencesHighlight": "off",
	"editor.stickyScroll.enabled": false,

	// folding
	"editor.foldingStrategy": "auto",

	// numbering
	"editor.lineNumbers": "off",
	"editor.glyphMargin": true,
	"editor.lightbulb.enabled": "off",

	// do not show cyrrilic highlights
	"editor.unicodeHighlight.ambiguousCharacters": false,

	// search
	"editor.find.autoFindInSelection": "multiline",
	
	// etc
	"editor.codeActionsOnSave": {},
	"editor.lineHeight": 0,
	"editor.emptySelectionClipboard": false,
	"editor.colorDecorators": false,
	"editor.bracketPairColorization.enabled": false,
	"editor.matchBrackets": "always",
	"editor.parameterHints.enabled": false,
	"editor.scrollbar.verticalScrollbarSize": 3,
	"editor.scrollbar.horizontal": "hidden",
	"editor.accessibilitySupport": "off",
	"editor.rename.enablePreview": false,
	"editor.renderLineHighlight": "none",
	"editor.guides.indentation": false,
	"editor.hover.enabled": false,
	"editor.semanticHighlighting.enabled": true,
	"editor.padding.top": 50,
	"editor.renderWhitespace": "all",
	"editor.gotoLocation.multipleDefinitions": "goto",
	"editor.folding": true,
	"editor.smoothScrolling": true,
	"editor.inlineSuggest.enabled": false,
	"editor.minimap.enabled": false,
	"editor.showFoldingControls": "never",

	// TERMINAL
	"terminal.integrated.tabs.enabled": false,
	"terminal.integrated.stickyScroll.enabled": false,
	"terminal.integrated.defaultProfile.osx": "zsh",
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.shellIntegration.decorationsEnabled": "never",

	// DIFF EDITOR
	"diffEditor.renderSideBySide": true,
	"diffEditor.ignoreTrimWhitespace": false,

	// EXTENSIONS
	// settings
	"extensions.ignoreRecommendations": true,
	"extensions.autoUpdate": true,

	// Project Manager
	"projectManager.sortList": "Recent",

	// Partial Diff
	"partialDiff.commandsOnContextMenu": {
		"markSection1": false,
		"markSection2AndTakeDiff": false
	},

	// Colorize (toggle in keybindings!)
	"colorize.include": ["**/*.json", "**/*.conf", "**/*.conf", "**/*.md"],
	"colorize.languages": ["css", "sass", "scss", "less", "postcss", "sss", "stylus", "xml", "svg", "toml", "json"],
	"colorize.enable_search_variables": true,
	"colorize.colorized_colors": ["BROWSERS_COLORS", "HEXA", "RGB", "HSL", "OKLAB", "OKLCH"],

	// Bookmarks
	"bookmarks.navigateThroughAllFiles": false,
	"bookmarks.gutterIconFillColor": "crimson",
	"bookmarks.gutterIconBorderColor": "rgb(99, 40, 64)",

	// Scroll Faster
	"scrollFaster.scrollByLines": 8,

	// Better Folding
	"betterFolding.showFoldedBodyLinesCount": false,

	// Outline Map
	"outline-map.defaultMaxDepth": 1,
	"outline-map.workspace.enabled": false,
	"outline-map.expand": "manual",
	"outline-map.follow": "manual",
	"outline-map.region.enabled": false,
	"outline-map.region.highlight": false,
	"outline-map.workspace.excludes": [],
	"redhat.telemetry.enabled": false,

	// Bracket Highlighter
	"BracketHighlighter.textColor": "#ac2e56",

	// Markdown All In One?
	"markdown.extension.toc.plaintext": true,
	"markdown.extension.print.theme": "dark",

	// Markdown Preview Enchanced
	"markdown-preview-enhanced.mermaidTheme": "dark",
	"markdown-preview-enhanced.breakOnSingleNewLine": false,
	"markdown-preview-enhanced.scrollSync": false,
	"markdown-preview-enhanced.codeBlockTheme": "github-dark.css",
	"markdown-preview-enhanced.enablePreviewZenMode": true,
	"markdown-preview-enhanced.previewTheme": "none.css",

	// Audio-preview
	"workbench.editorAssociations": {
		"*.oga": "wavPreview.audioPreview"
	},

	// Select by
	"selectby.regexes": {
		"SelectParagraph": {
			"flags": "m",
			"backward": "(\\r?\\n[ \\t]*\\r?\\n|^#+[ \\t].*\\r?\\n)",
			"forward": "(\\r?\\n[ \\t]*\\r?\\n|^#+[ \\t].*\\r?\\n)",
			"backwardInclude": false,
			"forwardInclude": false
		},
		"SelectBetweenHeadings": {
			"flags": "m",
			"backward": "^#+[ \\t].*\\r?\\n(?:[ \\t]*\\r?\\n)*",
			"forward": "(?:[ \\t]*\\r?\\n)*(?=^#+[ \\t])",
			"backwardInclude": false,
			"forwardInclude": false
		}
	},

	// Indented Block Higlighting #F55A8C
	"blockhighlight.background": ["200", "100", "255", ".04"],
	"blockhighlight.accentCurrentLine": false,
	"blockhighlight.isWholeLine": true,

	// Regex Robin
	"regexrobin.rules": [
		{
			"regex": ".*(?:A!|I!).*(?:\n.+)*",
			"regexFlags": { "multiline": true },
			"editor": [{ "color": "#bbbbbb" }]
		},
		{
			"regex": ".*AI!.*(?:\n.+)*",
			"regexFlags": { "multiline": true },
			"editor": [{ "color": "#F55A8C", "fontWeight": "bold" }]
		}
	],

	// Quick Dim
	"quickDim.opacity": 0.12,
	"applescript.showNotifications": false,
	"workbench.activityBar.location": "hidden"
}
